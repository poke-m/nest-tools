{
	"name": "@sophons/nest-tools",
	"version": "0.0.8",
	"author": "chogath",
	"license": "BSD-2-Clause",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"description": "üöç to provide the best components for nest.js server-side development",
	"keywords": [
		"nestjs",
		"nest",
		"ip"
	],
	"homepage": "https://github.com/sophons-space/nest-tools#readme",
	"bugs": {
		"url": "https://github.com/sophons-space/nest-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sophons-space/nest-tools"
	},
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"lint": "eslint \"{src,test}/**/*.ts\"",
		"update:version": "tsc && npm version patch && npm publish --access public"
	},
	"jest": {
		"testEnvironment": "node",
		"coverageDirectory": "coverage",
		"testRegex": "(/test/.spec.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.ts$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"./node_modules/(?!(lodash-es|other-es-lib))"
		]
	},
	"dependencies": {
		"@nestjs/common": "^7.6.12",
		"@types/express": "^4.17.8",
		"@types/uuid": "^8.3.0",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"rxjs": "^6.6.3",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@nestjs/core": "^7.6.12",
		"@nestjs/platform-express": "^7.6.12",
		"@types/jest": "^26.0.15",
		"@typescript-eslint/eslint-plugin": "^4.6.0",
		"@typescript-eslint/parser": "^4.6.0",
		"axios": "^0.21.1",
		"eslint": "^7.12.1",
		"jest": "^26.6.3",
		"reflect-metadata": "^0.1.13",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	}
}
